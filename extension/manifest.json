{
  "name": "SendUrl",
  "author": "Eugene Burmakin <http://frey.su>",
  "manifest_version": 2,
  "version": "0.1.1",
  "description": "Utilities to send tabs urls to backend.",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "homepage_url": "https://github.com/Freika/SendUrl",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon":  "icon/icon-48.png",
    "default_title": "Tab Urls",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  }
}
